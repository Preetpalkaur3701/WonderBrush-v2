[ FDirName $(wonderBrushSourceDir) gui ]
;




NAME = WonderBrush_qt ;
TARGET_LOCATION = $(WONDERBRUSH_QT_ROOT) ;
OBJECTS_LOCATION = $(WONDERBRUSH_QT_OBJECTS) ;






TYPE = APP ;









SRCS = $(SOURCE_FILES) ;



RSRCS = ;











LIBS = libbeqt.a libcommon_qt.a libbeqt.a libcommon_qt.a $(QT_LIBS) ;







LIBPATHS = $(QT_LIB_DIRS) ;




SYSTEM_INCLUDE_PATHS =
$(BEQT_INCLUDES)
$(LIBLAYOUT_INCLUDES)
$(QT_INCLUDES)
$(AGG_INCLUDES)
;




LOCAL_INCLUDE_PATHS =
$(SOURCE_DIRS)
[ CommonIncludes gui ]
[ CommonIncludes gui scrollview ]
[ CommonIncludes support ]

[ FDirName $(wonderBrushSourceDir) support ]
;






DEFINES = $(QT_DEFINES) ;
if $(ALLOCATION_CHECK) {
DEFINES += ALLOCATION_CHECK ;
}


COMPILER_FLAGS = -include $(BEQT_ROOT)/headers/BeOSBuildCompatibility.h ;
if $(ALLOCATION_CHECK) {
local debugDir = [ FDirName $(SUBDIR) debug ] ;
COMPILER_FLAGS += -include $(debugDir)/AllocationChecker.h ;
}



LINKER_FLAGS = ;
LINK on $(NAME) = g++ ;









TARGET_CONFIG = $(TARGET_PLATFORM) ;



rule Moc
{


local mocSource = $(1) ;
local header = $(2) ;

Depends $(mocSource) : $(header) ;
SEARCH on $(header) ?= $(SEARCH_SOURCE) ;

LOCATE on $(mocSource) ?= $(WONDERBRUSH_QT_OBJECTS) ;
MOC_DEFS on $(mocSource) = [ FDefines $(DEFINES) ] ;
MOC_INCLUDES on $(mocSource) = 
-I$(LOCAL_INCLUDE_PATHS) -I- -I$(SYSTEM_INCLUDE_PATHS)
;
}

actions Moc
{
/usr/local/Trolltech/Qt-4.0.1/bin/moc $(MOC_DEFS) $(MOC_INCLUDES) $(2) -o $(1)
}

MOC_HEADERS = src/BitmapView.h src/ZoomSlider.h ;
MOC_SOURCES = ;

local header ;
for header in $(MOC_HEADERS) {
header = $(header:BS) ;
local source = [ FGristFiles moc_$(header:BS=.cpp) ] ;
Moc $(source) : $(header) ;
MOC_SOURCES += $(source) ;
}

SRCS += $(MOC_SOURCES) ;


RunJamfileEngine ;
