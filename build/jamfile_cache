rule Moc
{


local mocSource = $(1) ;
local header = $(2) ;

Depends $(mocSource) : $(header) ;
SEARCH on $(header) ?= $(SEARCH_SOURCE) $(SOURCE_DIRS) ;

LOCATE on $(mocSource) ?= $(WONDERBRUSH_QT_OBJECTS) ;
MOC_DEFS on $(mocSource) = [ FDefines $(DEFINES) ] ;
MOC_INCLUDES on $(mocSource) = 
-I$(LOCAL_INCLUDE_PATHS) -I- -I$(SYSTEM_INCLUDE_PATHS)
;
}

actions Moc
{
/usr/local/Trolltech/Qt-4.0.1/bin/moc $(MOC_DEFS) $(MOC_INCLUDES) $(2) -o $(1)
}

MOC_HEADERS = MButton.h MCheckBox.h MenuFieldPrivate.h MenuPrivate.h
ScrollBar.h ;
MOC_SOURCES = ;

local header ;
for header in $(MOC_HEADERS) {
header = $(header:BS) ;
local source = [ FGristFiles moc_$(header:BS=.cpp) ] ;
Moc $(source) : $(header) ;
MOC_SOURCES += $(source) ;
}

SRCS += $(MOC_SOURCES) ;


RunJamfileEngine ;

ObjectC++Flags [ FGristFiles errors.o ] : -DBUILDING_HAIKU_ERROR_MAPPER=1 ;
