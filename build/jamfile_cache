rule SVGIcon
{
Depends "$(1)" : "$(2)" ;
}

actions SVGIcon
{
if [ -f /bin/IconTool ]
then
IconTool "$(1)" Build STD_ICON 1 -1 SVG "$(2)"
fi
}

local svgIconDir = [ FDirName $(WONDERBRUSH_ROOT) package ] ;
SVGIcon WonderBrush : $(svgIconDir)/Icon.svg ;
